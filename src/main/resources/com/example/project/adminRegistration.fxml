        <Button fx:id="reqReg" layoutX="317.0" layoutY="568.0" mnemonicParsing="false" onAction="#reqReg" prefHeight="34.0" prefWidth="189.0" styleClass="button" text="Request Registration" />

        <Button fx:id="backToMain" layoutX="471.0" layoutY="82.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="26.0" prefWidth="122.0" styleClass="buttonM" text="Main Menu" />
      <Label layoutX="107.0" layoutY="177.0" text="Enter UserName">
         <font>
            <Font name="ArtifaktElement-Black" size="16.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
