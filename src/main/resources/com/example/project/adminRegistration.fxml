<AnchorPane prefHeight="800.0" prefWidth="600.0" style="-fx-background-color: white;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.AdminRegistration">

    <children>
        <TextField fx:id="name" layoutX="249.0" layoutY="163.0" prefHeight="52.0" prefWidth="250.0" styleClass="text-field" />

        <TextField fx:id="pass" layoutX="249.0" layoutY="241.0" prefHeight="52.0" prefWidth="250.0" styleClass="text-field" />

        <TextField fx:id="confirmPass" layoutX="249.0" layoutY="334.0" prefHeight="52.0" prefWidth="250.0" styleClass="text-field" />

        <TextField fx:id="mobo" layoutX="249.0" layoutY="423.0" prefHeight="52.0" prefWidth="250.0" styleClass="text-field" />
        <Label layoutX="109.0" layoutY="253.0" text="Enter Password">
         <font>
            <Font name="ArtifaktElement-Black" size="16.0" />
         </font></Label>
        <Label layoutX="91.0" layoutY="348.0" text="Confirm Password">
         <font>
            <Font name="ArtifaktElement-Black" size="16.0" />
         </font></Label>
        <Label layoutX="70.0" layoutY="435.0" text="Enter Mobile Number">
         <font>
            <Font name="ArtifaktElement-Black" size="16.0" />
         </font></Label>
        <Label layoutX="170.0" layoutY="125.0" text="NOTE : USERNAME MUST BE OF 10 CHARACTERS!!!" textFill="#e70707" />
