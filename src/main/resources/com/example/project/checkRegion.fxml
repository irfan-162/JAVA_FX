    <!-- VBox for content in the center -->
    <VBox fx:id="container" alignment="CENTER" layoutX="50.0" layoutY="50.0" prefHeight="400.0" prefWidth="600.0" spacing="20.0" />

    <!-- Button in the bottom-right corner -->
    <Button fx:id="returnToMain" layoutX="451.0" layoutY="499.0" mnemonicParsing="false" onAction="#returnToMain" prefHeight="43.0" prefWidth="122.0" styleClass="buttonM" text="Main Menu" />

</AnchorPane>
