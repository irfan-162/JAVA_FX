<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.Result">

    <children>
        <ListView fx:id="ResultListView" layoutX="209.0" layoutY="40.0" prefHeight="200.0" prefWidth="200.0" styleClass="result-list" />

        <TextArea fx:id="resultContentArea" editable="false" layoutX="99.0" layoutY="280.0" prefHeight="162.0" prefWidth="400.0" styleClass="result-text" wrapText="true" />

        <Button fx:id="adminMenu" layoutX="14.0" layoutY="520.0" mnemonicParsing="false" onAction="#returnToAdmin" prefHeight="43.0" prefWidth="134.0" styleClass="buttonM" text="Admin Menu">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>

        <Button layoutX="448.0" layoutY="520.0" mnemonicParsing="false" onAction="#returnToMain" prefHeight="43.0" prefWidth="122.0" styleClass="buttonM" text="Main Menu">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>

        <Label fx:id="winner" alignment="CENTER" contentDisplay="CENTER" layoutX="218.0" layoutY="461.0" prefHeight="18.0" prefWidth="162.0" styleClass="winner-label" text="Label" />

        <Label fx:id="resLabel" alignment="TOP_CENTER" layoutX="143.0" layoutY="316.0" prefHeight="126.0" prefWidth="314.0" styleClass="result-label" text="Select a result">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
