<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.VotingController">
   <children>
      <ListView fx:id="availableC" layoutX="45.0" layoutY="189.0" prefHeight="133.0" prefWidth="200.0" styleClass="list-view" />
      <ListView fx:id="selectedC" layoutX="343.0" layoutY="189.0" prefHeight="134.0" prefWidth="200.0" styleClass="list-view" />

      <Label layoutX="39.0" layoutY="139.0" text="Available Candidates :">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Label>
      <Label layoutX="344.0" layoutY="139.0" text="Selected Candidates :">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Label>

      <Button fx:id="startVote" layoutX="196.0" layoutY="599.0" mnemonicParsing="false" onAction="#startVote" prefHeight="48.0" prefWidth="199.0" styleClass="button" text="Start Vote">
         <font>
            <Font name="Arial Rounded MT Bold" size="25.0" />
         </font>
      </Button>

      <Label layoutX="230.0" layoutY="442.0" prefHeight="27.0" prefWidth="132.0" text="Select Duration :">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </Label>

      <TextField fx:id="hours" layoutX="144.0" layoutY="483.0" prefHeight="30.0" prefWidth="54.0" />
      <TextField fx:id="minutes" layoutX="295.0" layoutY="483.0" prefHeight="30.0" prefWidth="54.0" />
      <TextField fx:id="seconds" layoutX="471.0" layoutY="483.0" prefHeight="30.0" prefWidth="54.0" />

      <Label layoutX="68.0" layoutY="490.0" prefHeight="27.0" prefWidth="68.0" text="Hours :">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="206.0" layoutY="490.0" prefHeight="27.0" prefWidth="85.0" text=" Minutes :">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="368.0" layoutY="490.0" prefHeight="27.0" prefWidth="86.0" text="Seconds :">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </Label>

      <Button fx:id="selectButton" layoutX="103.0" layoutY="338.0" mnemonicParsing="false" onAction="#selectButton" styleClass="button" text="Select" />
      <Button fx:id="removeButton" layoutX="395.0" layoutY="338.0" mnemonicParsing="false" onAction="#removeButton" styleClass="button" text="Remove" />

      <Label layoutX="197.0" layoutY="456.0">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Label>

      <Label fx:id="timer" layoutX="578.0" layoutY="687.0" prefHeight="18.0" prefWidth="0.0" text="Label" />

      <Label fx:id="labelRegion" layoutX="180.0" layoutY="553.0" prefHeight="30.0" prefWidth="232.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>

      <Button layoutX="395.0" layoutY="728.0" mnemonicParsing="false" onAction="#backAdmin" prefHeight="27.0" prefWidth="174.0" styleClass="buttonM" text="Admin Menu" />
   </children>
</AnchorPane>
